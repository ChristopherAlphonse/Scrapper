{
  "name": "scrapper",
  "version": "1.0.0",
  "main": "dist",
  "license": "MIT",
  "volta": {
    "node": "19.5.0"
  },
  "scripts": {
    "start": "tsc && node dist/index.js",
    "dev": "npx concurrently -k -n COMPILER,NODEMON -c red,blue \"npx tsc -w\"  \"nodemon -w dist -q dist/index.js\" ",
    "check": "tsc --noEmit && eslint src --ext .ts",
    "prepare": "husky install",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 80,
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {

    "@types/axios": "^0.14.0",
    "@types/cheerio": "0.22.35",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node-fetch": "^2.6.11",
    "axios": "^1.6.8",
    "cheerio": "1.0.0-rc.12",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "node-fetch": "^3.3.2",
    "os": "^0.1.2"
  },
  "devDependencies": {
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.4.13",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
