{
  "name": "scrapper",
  "version": "1.0.0",
  "main": "dist",
  "license": "MIT",
  "volta": {
    "node": "19.5.0"
  },
  "scripts": {
    "start": "tsc && node dist/index.js",
    "dev": "npx concurrently -k -n COMPILER,NODEMON -c red,blue \"npx tsc -w\"  \"nodemon -w dist -q dist/index.js\" ",
    "check": "tsc --noEmit && eslint src --ext .ts",
    "prepare": "husky install",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 80,
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {
    "@prisma/client": "^4.13.0",
    "@types/axios": "^0.14.0",
    "@types/cheerio": "0.22.31",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node-fetch": "^2.6.3",
    "axios": "^1.3.6",
    "cheerio": "1.0.0-rc.12",
    "cluster": "^0.7.7",
    "concurrently": "^8.0.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "node-fetch": "^3.3.1",
    "os": "^0.1.2"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.53",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
