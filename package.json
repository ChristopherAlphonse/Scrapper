  },
  "devDependencies": {
    "typescript": "^5.0.4",
    "ts-node": "^10.9.1",
    "cheerio": "1.0.0-rc.12",
    "node-fetch": "^3.3.1",
    "@swc/cli": "^0.1.60",
    "@swc/core": "^1.3.44",
    "@types/node": "^18.15.10",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
